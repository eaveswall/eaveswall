  "description": "A simple blog for us",
  "version": "0.1.0",
  "author": {
    "name": "The Eaveswall contributors"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^18.0.0",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@mdx-js/mdx": "^1.6.1",
    "@mdx-js/react": "^1.6.1",
