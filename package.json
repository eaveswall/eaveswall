    "gatsby-transformer-remark": "^2.6.58",
    "gatsby-transformer-sharp": "^2.5.0",
    "netlify-cms-app": "^2.12.12",
    "node-sass": "^4.14.1",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-instantsearch-dom": "^6.4.0",
    "resolve-url-loader": "^3.1.1",
    "styled-components": "^5.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "netlify-lambda": "^1.6.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-beta": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lambda:build": "netlify-lambda build functions/",
    "lambda:serve": "netlify-lambda serve functions/",
    "prebuild": "cd functions && npm install",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "svg-icon": "svgo --config src/images/svg/svgo.yml -i src/images/svg/eaveswall-icon.inline.svg",
    "path-clip": "svgo --config src/images/svg/svgo.yml -i src/images/svg/clip-path.inline.svg",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calebpitan/eaveswall"
  },
  "bugs": {
    "url": "https://github.com/calebpitan/eaveswall/issues"
  }
}
